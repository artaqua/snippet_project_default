include pug/mixins/svg-icon.pug

doctype html
html.no-js
	head
		include pug/meta.pug
		title Home
		include pug/styles.pug
	body
		include pug/old-ie.pug
		include pug/svg-sprite.pug

		include pug/_includes/_page-loading.pug

		include pug/_includes/_header.pug

		main
			.container-fluid
				section.any
					p Main.

					// SvgSprite
					+svg('close')
					+svg('shape')
					// Iconfont
					i.icon.icon-test
					// PngSprite
					i.icon-arrow

		include pug/_includes/_footer.pug

		include pug/scripts.pug
	

			

